{
    "name": "example",
    "version": ${version},
    "containers": [
        {
            "name": "httpd",
            "systemd": {
                "unit-files": [
                    "httpd.service"
                ]
            }
        }
    ],
    "images": [
        {
            "id": "sha512-${sha512}",
            "path": "image.aci"
        }
    ],
    "volumes": [
        {
            "name": "www",
            "path": "/var/www",
            "read-only": true,
            "data": "data.tgz"
        }
    ]
}
